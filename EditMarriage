    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       
        String male= jTextField1.getText();
        String female= jTextField2.getText();
        String year= jTextField3.getText();
        LoadOntology B =new LoadOntology();
        B.setModifiedMarriageValues(ont,manager,male, female, year, mLabel ,model,a,b,c);
        this.dispose();
    }                                        

