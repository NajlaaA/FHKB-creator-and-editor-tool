    public void setValues (OWLOntology o,OWLOntologyManager mang,String year, String male, String female,String label,DefaultListModel m)
    {
        jTextField1.setText(male);
      jTextField2.setText(female);
      jTextField3.setText(year);
      a=male; b=female; c=year;
      model=m;
      mLabel=label;
      ont=o;
      manager=mang;
    }

