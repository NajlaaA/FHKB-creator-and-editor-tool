    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String first1= jTextField1.getText();
        String last1= jTextField3.getText();
        String birth1=jTextField2.getText();
        String death1=jTextField4.getText();
       
        LoadOntology A =new LoadOntology();
       
        A.setParentValues(ont,manager,child, label,first1,last1, birth1, death1, model,cls,childT,tableModel, preFirst, preLast, preBirth, preDeath,tree);
        this.dispose();
    }                                        

