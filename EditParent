     public void setValues(OWLOntology o,OWLOntologyManager mang,String childLabel, String lab,String first, String last, String birth, String death,int classType,String childType ,DefaultListModel m,DefaultTableModel tmodel,FamilyTree t)
    {
      jTextField1.setText(first);
      jTextField3.setText(last);
      jTextField2.setText(birth);
      jTextField4.setText(death);
      preFirst=first;
      preLast= last;
      preBirth=birth;
      preDeath=death;
      tableModel=tmodel;
      label=lab;
      child= childLabel;
      model=m;
      cls= classType;
      childT= childType;
      ont=o;   manager=mang;
      tree=t;
      setDefaultCloseOperation(DISPOSE_ON_CLOSE);
      
    }

