    public void setValues(OWLOntology o,OWLOntologyManager x,String lab,String first, String last, String birth, String death, String mLabel, String fLabel,String classType, DefaultListModel m, DefaultTableModel tmodel, FamilyTree t)
    {
      jTextField1.setText(first);
      jTextField2.setText(last);
      jTextField3.setText(birth);
      jTextField4.setText(death);
      oldFirst=first;
      oldLast=last;
      oldBirth=birth;
      oldDeath=death;
      tree=t;
     
      ont=o;
      manager=x;
      label=lab;
      mother= mLabel;
      father= fLabel;
      model=m;
      tableModel=tmodel;
      if(classType.equals("Woman"))
        femaleRButton.setSelected(true);
      else if(classType.equals("Man"))
        maleRButton.setSelected(true);
      setDefaultCloseOperation(DISPOSE_ON_CLOSE);
      
    }
   
