    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
     
        first1= jTextField1.getText();
        last1= jTextField2.getText();
        birth1=jTextField3.getText();
        death1=jTextField4.getText();
        if(maleRButton.isSelected())
            cls=1;
        else if(femaleRButton.isSelected())
           cls=2;
        LoadOntology A =new LoadOntology();
       
        A.setModifyValues(ont,manager,label,first1,last1, birth1, death1,model,cls,tableModel,oldFirst,oldLast,oldBirth,oldDeath,tree);
        this.dispose();
    }                                        

